<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
       <record id="gemini_ai_partner" model="res.partner">
            <field name="name">Asistente AI Gemini</field>
            <field name="email">geminiasistant@gmail.com</field>
            <field name="active" eval="True"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="im_status">online</field>
            <field name="type">other</field>
            <field name="is_company">False</field>
            <field name="partner_share">False</field>
        </record>
        
        <record id="gemini_ai_user" model="res.users">
            <field name="partner_id" ref="gemini_ai_partner"/>
            <field name="login">gemini_bot</field>
            <field name="password">gemini_bot</field>
            <field name="groups_id" eval="[(6, 0, [
                ref('base.group_user'),
                ref('base.group_system')
            ])]"/>
            <field name="company_ids" eval="[(6, 0, [ref('base.main_company')])]"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="active" eval="True"/>
            <field name="notification_type">email</field>
            <field name="share">False</field>
            <field name="im_status">online</field>
        </record>
    </data>
</odoo>