<odoo>
    <record id="view_channel_form_inherit_gemini" model="ir.ui.view">
        <field name="name">discuss.channel.form.gemini</field>
        <field name="model">discuss.channel</field>
        <field name="inherit_id" ref="mail.discuss_channel_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Gemini" name="gemini">
                    <group>
                        <field name="is_gemini_enabled"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
